	'CROSSD'																, ; //XB_ALIAS
	'5'																		, ; //XB_TIPO
	'01'																	, ; //XB_SEQ
	''																		, ; //XB_COLUNA
	''																		, ; //XB_DESCRI
	''																		, ; //XB_DESCSPA
	''																		, ; //XB_DESCENG
	'M->ZX_DOC'																} ) //XB_CONTEM

//
// Consulta SZW
//
aAdd( aSXB, { ;
	'SZW'																	, ; //XB_ALIAS
	'1'																		, ; //XB_TIPO
	'01'																	, ; //XB_SEQ
	'DB'																	, ; //XB_COLUNA
	'Crossdocking'															, ; //XB_DESCRI
	'Crossdocking'															, ; //XB_DESCSPA
	'Crossdocking'															, ; //XB_DESCENG
	'SZW'																	} ) //XB_CONTEM

aAdd( aSXB, { ;
	'SZW'																	, ; //XB_ALIAS
	'2'																		, ; //XB_TIPO
	'01'																	, ; //XB_SEQ
	'02'																	, ; //XB_COLUNA
	'Processo Imp'															, ; //XB_DESCRI
	'Processo Imp'															, ; //XB_DESCSPA
	'Processo Imp'															, ; //XB_DESCENG
	''																		} ) //XB_CONTEM

aAdd( aSXB, { ;
	'SZW'																	, ; //XB_ALIAS
	'4'																		, ; //XB_TIPO
	'01'																	, ; //XB_SEQ
	'01'																	, ; //XB_COLUNA
	'Processo Imp'															, ; //XB_DESCRI
	'Processo Imp'															, ; //XB_DESCSPA
	'Processo Imp'															, ; //XB_DESCENG
	'ZW_PROIMP'																} ) //XB_CONTEM

aAdd( aSXB, { ;
	'SZW'																	, ; //XB_ALIAS
	'4'																		, ; //XB_TIPO
	'01'																	, ; //XB_SEQ
	'02'																	, ; //XB_COLUNA

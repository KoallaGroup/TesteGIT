				LOOP

			CASE ( nKey == 281)
				ExecBlock("BROC020J",.F.,.F.)
				LOOP

			CASE ( nKey == -4)
				Shell()
				LOOP

			CASE ( nKey == -8)
				Utils()
				LOOP

			ENDCASE
		ENDIF

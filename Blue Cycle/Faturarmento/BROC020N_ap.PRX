					nKEY :=27
					EXIT
				ELSEIF M_but == 1
 					IF nArea == 2       // Rodape do Term Vertical
						nKey := 24
						EXIT
					ELSEIF nArea == 1   // Topo do Term Vertical
						nKey := 5
						EXIT
					ENDIF
 	
				ELSEIF M_but == 2
					IF nArea == 1
						nKey := 13	
						EXIT
					EndIF
					IF nArea > 10
						EXIT
					ENDIF
				ENDIF
			ENDIF	
		EndDo
		//ÚÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄÄ¿

// Substituido pelo assistente de conversao do AP6 IDE em 25/06/04 ==> Function MC050NSkip  //()
Static Function MC050NSkip  //()
nCnt := 0
If nPula > 0
	While nCnt < nPula
		nPosBco:=nPosBco+1
		Skip 1
		If Eof() 
			nPosBco:=nPosBco - 1
			Skip -1
			Exit
		EndIf
		nCnt:=nCnt+1
	EndDo
Else
	While nCnt > nPula
		nPosBco:=nPosBco - 1
		Skip -1
		If Bof()
			nPosBco:=nPosBco + 1
			Exit
		Endif
		nCnt:=nCnt-1
	EndDo
EndIf

user Function VLCODSERIE()
	dbSelectArea("SZ6")
	dbSetOrder(1)
	if dbSeek(xFilial("SZ6")+M->Z6_CODIGO)
		msgAlert ("Já existe uma Série com esse código !!")
		return .F.
	endif
return .T.
